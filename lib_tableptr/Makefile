# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  crc32_config = debug
endif
ifeq ($(config),release)
  crc32_config = release
endif

PROJECTS := crc32

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

crc32:
ifneq (,$(crc32_config))
	@echo "==== Building crc32 ($(crc32_config)) ===="
	@${MAKE} --no-print-directory -C . -f crc32.make config=$(crc32_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f crc32.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   crc32"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"